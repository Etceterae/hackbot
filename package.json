{
  "name": "hackbot",
  "version": "0.0.0",
  "private": true,
  "author": "RHL",
  "description": "Hack all the things!",
  "dependencies": {
    "coffee-script": "^1.12.6",
    "hubot": "^2.19.0",
    "hubot-auth": "^2.1.0",
    "hubot-diagnostics": "0.0.1",
    "hubot-friendly": "0.0.1",
    "hubot-google-images": "^0.2.6",
    "hubot-google-translate": "^0.2.0",
    "hubot-help": "^0.2.0",
    "hubot-maps": "0.0.2",
    "hubot-pugme": "^0.1.0",
    "hubot-reboot": "^1.2.0",
    "hubot-redis-brain": "0.0.3",
    "hubot-robotstuff": "0.0.1",
    "hubot-rules": "^0.1.1",
    "hubot-scripts": "^2.17.2",
    "hubot-shipit": "^0.2.0",
    "hubot-slack": "^4.3.4",
    "hubot-test-helper": "^1.5.1",
    "jest": "^20.0.4",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "testdouble": "^2.1.2"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": "0.10.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee"
    ],
    "setupTestFrameworkScriptFile":"<rootDir>/test/setup.js",
    "testMatch": [
      "**/__tests__/**/*.(js|coffee)?(x)",
      "**/?(*.)(spec|test).(js|coffee)"
    ],
    "transform": {
      ".*":"<rootDir>/test/preprocessor.js"
    }
  }
}
